<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="470.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Withdraw">
   <children>
      <Label layoutX="49.0" layoutY="51.0" text="Account Number :">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="44.0" layoutY="93.0" text="Withdraw Amount :">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="acNum" layoutX="214.0" layoutY="52.0" prefHeight="25.0" prefWidth="206.0" promptText="Enter Your Account Number" />
      <TextField fx:id="wAmount" layoutX="214.0" layoutY="94.0" prefHeight="25.0" prefWidth="206.0" promptText="You Can Check Your Balance First" />
      <Button layoutX="351.0" layoutY="181.0" mnemonicParsing="false" onAction="#enter" text="Enter">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <TextArea fx:id="information" layoutX="42.0" layoutY="229.0" prefHeight="171.0" prefWidth="379.0" promptText="Enter Your Account Number and Password and Press &quot;Balance Check&quot; button  to Check  Your Balance">
         <font>
            <Font size="17.0" />
         </font></TextArea>
      <Button layoutX="44.0" layoutY="181.0" mnemonicParsing="false" onAction="#exit" text="Exit">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Label layoutX="44.0" layoutY="131.0" text="Password :">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <PasswordField fx:id="pass" layoutX="214.0" layoutY="132.0" prefHeight="25.0" prefWidth="206.0" />
      <Button layoutX="176.0" layoutY="181.0" mnemonicParsing="false" onAction="#checkBalance" prefHeight="39.0" prefWidth="107.0" text="Balance Check">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
