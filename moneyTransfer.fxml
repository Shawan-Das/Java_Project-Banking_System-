<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="482.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.moneyTransfer">
   <children>
      <Label layoutX="67.0" layoutY="36.0" text="Account Number :">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="67.0" layoutY="72.0" text="Transfer To :">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="57.0" layoutY="106.0" text="Transfer Amount :">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="74.0" layoutY="141.0" text="Password :">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="frstAccount" layoutX="218.0" layoutY="38.0" prefHeight="25.0" prefWidth="191.0" promptText="Enter Your Account Number" />
      <TextField fx:id="secAccount" layoutX="218.0" layoutY="73.0" prefHeight="25.0" prefWidth="191.0" promptText="Account Number you wish to transfer Money" />
      <TextField fx:id="amount" layoutX="218.0" layoutY="107.0" prefHeight="25.0" prefWidth="191.0" promptText="Amount you want to transfer" />
      <PasswordField fx:id="password" layoutX="218.0" layoutY="142.0" prefHeight="25.0" prefWidth="191.0" />
      <Button layoutX="345.0" layoutY="182.0" mnemonicParsing="false" onAction="#enter" prefHeight="36.0" prefWidth="64.0" text="Enter">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Button layoutX="84.0" layoutY="182.0" mnemonicParsing="false" onAction="#back" prefHeight="36.0" prefWidth="64.0" text="Cancle">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <TextArea fx:id="information" layoutX="68.0" layoutY="221.0" prefHeight="179.0" prefWidth="347.0" promptText="If you want to Check your Balance first, Enter your Account Number &amp; Password and press &quot;Balance Check&quot; Button">
         <font>
            <Font size="17.0" />
         </font></TextArea>
      <Button layoutX="195.0" layoutY="182.0" mnemonicParsing="false" onAction="#checkBalance" prefHeight="36.0" prefWidth="106.0" text="Balance Check">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
