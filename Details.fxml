<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="283.0" prefWidth="412.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Details">
   <children>
      <TextArea fx:id="information" layoutY="-1.0" prefHeight="239.0" prefWidth="409.0" promptText="Press Start Button To see Account Details.">
         <font>
            <Font size="18.0" />
         </font></TextArea>
      <Button layoutY="239.0" mnemonicParsing="false" onAction="#start" prefHeight="42.0" prefWidth="80.0" text="Show">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
      <Button layoutX="173.0" layoutY="239.0" mnemonicParsing="false" onAction="#back" prefHeight="42.0" prefWidth="67.0" text="Exit">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
      <Button layoutX="329.0" layoutY="227.0" mnemonicParsing="false" onAction="#next" prefHeight="25.0" prefWidth="80.0" text="&gt;&gt;">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button layoutX="329.0" layoutY="257.0" mnemonicParsing="false" onAction="#previous" prefHeight="25.0" prefWidth="80.0" text="&lt;&lt;">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
